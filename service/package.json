{
  "name": "ggtech-service",
  "version": "0.1",
  "main": "build/api.js",
  "private": true,
  "scripts": {
    "build": "concurrently -c \"bgBlue.bold,bgGreen.bold\" -n ts,graphql \"npm:build:ts\" \"npm:build:graphql\"",
    "build:ts": "tsc -p tsconfig.json",
    "build:graphql": "cpx \"src/graphql/**/*.graphql\" build/graphql",
    "lint": "eslint --ext=jsx,ts,tsx src",
    "lint-fix": "eslint --fix --ext=jsx,ts,tsx src",
    "prettier": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "migrate": "PROCESS=migrate env-cmd node build/api.js",
    "prebuild": "shx rm -rf build/",
    "start": "PORT=4000 env-cmd nodemon build/api.js"
  },
  "keywords": [],
  "author": "",
  "dependencies": {
    "@graphql-tools/load-files": "^6.6.1",
    "@graphql-tools/merge": "^8.3.14",
    "apollo-server-core": "^3.6.3",
    "apollo-server-express": "^3.6.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.4.0",
    "lodash": "^4.17.21",
    "objection": "^3.0.1",
    "pg": "^8.10.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.18",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "concurrently": "^7.6.0",
    "cpx2": "^4.2.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-watch": "^8.0.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.2",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
  }
}
